{
  "build": {
    "markdownEngineName": "dfm-latest",
    "content": [
      {
        "files": [
          "api/**/*.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs",
        "dest": ".",
        "exclude": [
          "api/overwrites/**",
          "samples/**/*.*",
          "**/includes/**",
          "***/contributing.md"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "_csharplang/spec",
        "dest": "csharp/language-reference/language-specification",
        "exclude": [
          "README.md",
          "LICENSE.md",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "_vblang/spec",
        "dest": "visual-basic/reference/language-specification",
        "exclude": [
          "README.md",
          "LICENSE.md",
          "**/includes/**"
        ]
      },
      {
        "group": "netcore",
        "files": [
          "core/**/*.md",
          "core/**/*.yml"
        ]
      },
      {
        "files": [
          "index.md"
        ]
      },
      {
        "files": "toc.yml",
        "dest": "breadcrumb"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "src": "docs",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "samples/**/*.*",
          "_themes/DocPacker/**"
        ]
      },
      {
        "group": "netcore",
        "src": "docs/core",
        "files": [
          "images/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp"
        ],        
        "exclude": [
          "**/obj/**",
          "samples/**/*.*",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/dotnet/breadcrumb/toc.json",
      "_displayLangs": [
        "csharp"
      ],
      "author": "dotnet-bot",
      "ms.author": "dotnetcontent",
      "manager": "wpickett",
      "searchScope": [".NET"],
      "uhfHeaderId": "MSDocsHeader-DotNet",
      "apiPlatform": "dotnet",
      "ms.topic": "article"
    },
    "fileMetadata": {
      "ms.prod": {
        "_csharplang/spec/*.md": ".net",
        "_vblang/spec/*.md": ".net"
      },
      "ms.topic": {
        "_csharplang/spec/*.md": "language-reference",
        "_vblang/spec/*.md": "language-reference"
     },
      "dev_langs": {
        "docs/core/**/**.md": ["csharp"],
        "docs/csharp/**/**.md": ["csharp"],
        "docs/framework/**/**.md": ["csharp"],
        "docs/fsharp/**/**.md": ["fsharp"],
        "docs/standard/**/**.md": ["csharp"],
        "docs/visual-basic/**/**.md": ["vb"]
      },
      "ms.author": {
        "_csharplang/spec/*.md": "wiwagn",
        "_vblang/spec/*.md": "wiwagn"
      },
      "ms.technology": {
        "_csharplang/spec/*.md": "devlang-csharp",
        "csharp/quick-starts/**": "csharp-interactive",
        "_vblang/spec/*.md": "devlang-visual-basic"
      },
      "ms.date": {
        "_csharplang/spec/*.md": "07/01/2017",
        "_vblang/spec/*.md": "07/21/2017"
      }
    },
    "dest": "_site",
    "template": [
      "docs.html"
    ],
    "groups": {
    "netcore": {      
      "moniker_range": ">= netcore-1.0"
    }
  },
    "xref": [
      "_zip/office15.zip",
      "_zip/missingapi.yml"
    ],
    "lruSize": 0
  }
}
