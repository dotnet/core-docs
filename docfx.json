{
  "build": {
    "markdownEngineName": "dfm-latest",
    "content": [
      {
        "files": [
          "api/**/*.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs",
        "dest": ".",
        "exclude": [
          "api/overwrites/**",
          "samples/**/*.*",
          "**/includes/**",
          "***/contributing.md"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "_csharplang/spec",
        "dest": "csharp/language-reference/language-specification",
        "exclude": [
          "README.md",
          "LICENSE.md",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "_vblang/spec",
        "dest": "visual-basic/reference/language-specification",
        "exclude": [
          "README.md",
          "LICENSE.md",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "core/**/*.md",
          "core/**/*.yml"
        ]
      },
      {
        "files": [
          "index.md"
        ]
      },
      {
        "files": "toc.yml",
        "dest": "breadcrumb"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "src": "docs",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "samples/**/*.*",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/dotnet/breadcrumb/toc.json",
      "_displayLangs": ["csharp"],
      "dev_langs": ["csharp"],
      "ms.devlang": "dotnet",
      "author": "dotnet-bot",
      "ms.author": "dotnetcontent",
      "manager": "wpickett",
      "searchScope": [".NET"],
      "uhfHeaderId": "MSDocsHeader-DotNet",
      "apiPlatform": "dotnet",
      "ms.topic": "conceptual",
      "feedback_system": "GitHub",
      "feedback_github_repo": "dotnet/docs",
      "feedback_product_url": "https://developercommunity.visualstudio.com/spaces/61/index.html" 
    },
    "fileMetadata": {
      "feedback_product_url": {
        "docs/fsharp/**/**.md": "https://github.com/Microsoft/visualfsharp"
      },
      "ms.prod": {
        "_csharplang/spec/*.md": "dotnet-csharp",
        "_vblang/spec/*.md": "dotnet-visualbasic",
        "docs/core/**/**.md": "dotnet-core",
        "docs/csharp/**/**.md": "dotnet-csharp",
        "docs/framework/**/**.md": "dotnet-framework",
        "docs/fsharp/**/**.md": "dotnet-fsharp",
        "docs/standard/**/**.md": "dotnet",
        "docs/visual-basic/**/**.md": "dotnet-visualbasic"
      },
      "ms.topic": {
        "_csharplang/spec/*.md": "language-reference",
        "_vblang/spec/*.md": "language-reference"
      },
      "dev_langs": {
        "docs/fsharp/**/**.md": ["fsharp"],
        "docs/visual-basic/**/**.md": ["vb"]
      },
      "ms.devlang": {
        "docs/csharp/**/**.md": "csharp",
        "docs/fsharp/**/**.md": "fsharp",
        "docs/visual-basic/**/**.md": "vb"
      },
      "author": {
        "docs/core/**/**.md": "mairaw",
        "docs/csharp/**/**.md": "billwagner",
        "docs/fsharp/**/**.md": "cartermp"
      },
      "ms.author": {
        "_csharplang/spec/*.md": "wiwagn",
        "_vblang/spec/*.md": "wiwagn",
        "docs/core/**/**.md": "mairaw",
        "docs/csharp/**/**.md": "wiwagn",
        "docs/fsharp/**/**.md": "phcart"
      },
      "ms.date": {
        "_csharplang/spec/*.md": "07/01/2017",
        "_vblang/spec/*.md": "07/21/2017"
      }
    },
    "dest": "_site",
    "template": [
      "docs.html"
    ],
    "xref": [
      "_zip/office15.zip",
      "_zip/missingapi.yml"
    ],
    "lruSize": 0
  }
}